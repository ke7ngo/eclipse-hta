<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hta SYSTEM "huppaal-0.6.dtd">
<hta>
	<template type="XOR">
		<name>Template</name>
		<entry id="Template.ENTRY">
			<name>ENTRY</name>
			<connection>
				<target ref="Template.A.0" />
			</connection>
		</entry>
		<exit id="Template.EXIT6">
			<name>EXIT</name>
		</exit>
		<location id="Template.A.0">
			<name>A</name>
			<label kind="invariant" />
		</location>
		<location id="Template.B.5">
			<name>B</name>
			<label kind="invariant" />
		</location>
		<component id="Template.1" instantiates="SD">
			<name>N2</name>
		</component>
		<transition>
			<source exitref="SD.EXIT4" ref="Template.1" />
			<target ref="Template.B.5" />
		</transition>
		<transition>
			<source ref="Template.A.0" />
			<target entryref="SD.ENTRY" ref="Template.1" />
			<label kind="guard" />
			<label kind="assignment" />
			<label kind="synchronisation" />
		</transition>
	</template>
	<template type="XOR">
		<name>SD</name>
		<entry id="SD.ENTRY">
			<name>ENTRY</name>
			<connection>
				<target ref="SD.S.3" />
			</connection>
		</entry>
		<exit id="SD.EXIT4">
			<name>EXIT</name>
		</exit>
		<location id="SD.S.3">
			<name>S</name>
			<label kind="invariant" />
		</location>
		<transition>
			<source ref="SD.S.3" />
			<target ref="SD.EXIT4" />
			<label kind="guard" />
			<label kind="assignment" />
			<label kind="synchronisation" />
		</transition>
	</template>
	<instantiation>template := Template();</instantiation>
	<system>system template;</system>
	<globalinit ref="Template.ENTRY" instantiationname="template" />
</hta>